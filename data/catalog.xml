<?xml version="1.0" encoding="UTF-8"?>
<catalog name="as">


   <test-case name="docbook-tests-001" xslt-version="2.0">
      <description>
         Shows a docbook file transformed into HTML format.
      </description>
      <created by="Debbie Lockett" on="2014-03-03"/>
      <test>
         <stylesheet file="docbook-tests/docbook/xsl/html/docbook_custom.xsl"/>
         <source file="docbook-tests/prague2011mhk.xml"/>
      </test>
      <result>
         <assert>
            count(/html/body//div)&gt; 10
         </assert>
      </result>
   </test-case>
   
   <test-case name="docbook-tests-002" xslt-version="2.0">
      <description>
         Calculate word frequencies.
      </description>
      <created by="Debbie Lockett" on="2014-03-03"/>
      <test>
         <stylesheet file="docbook-tests/wordcountex.xsl"/>
         <source file="docbook-tests/prague2011mhk.xml"/>
      </test>
      <result>
         <assert>
            /wordcount/word[@word="the"]/@frequency="331"
         </assert>
      </result>
   </test-case>
   
   <test-case name="musicxml-tests-001">
      <description>
         Music xml partwise to timewise
      </description>
      <created by="Debbie Lockett" on="2014-03-04"/>
      <test>
         <stylesheet file="musicxml-tests/parttime.xsl"/>
         <source file="musicxml-tests/SchbAvMaSample.xml"/>
      </test>
      <result>
         <assert>
            /score-timewise/@version="3.0"
         </assert>
      </result>
   </test-case>
   
