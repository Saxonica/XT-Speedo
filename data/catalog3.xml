   <test-case name="map-wordmap1-1" xslt-version="3.0">
      <description>
         Count size of map from distinct words to set of elements containing them.
      </description>
      <created by="Debbie Lockett" on="2014-04-01"/>
      <test>
         <stylesheet file="map-tests/wordmap1.xsl"/>
         <source file="xmark-tests/xmark1.xml"/>
      </test>
      <result>
         <assert>            
            //*[1] = '15927'
         </assert>
      </result>
   </test-case>
   
