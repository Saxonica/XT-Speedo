   <test-case name="map-numbermap1" xslt-version="3.0">
      <description>
         Create an integer map (independent of source file) related to Fibonacci sequence 
         (recursive), and change all values in the map.
         Map has 10000 pairs, only first 100 are printed in output.
      </description>
      <created by="Debbie Lockett" on="2014-04-02"/>
      <test>
         <stylesheet file="map-tests/numbermap1.xsl"/>
         <source file="xsltmark-tests/axis.xml"/>
      </test>
      <result>
         <assert>            
            //numbermap[@key = '100']/@value = '6'
         </assert>
      </result>
   </test-case>
   
